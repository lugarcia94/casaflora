.product__specification
    max-width: 1320px
    width: 100%
    padding: 0 15px
    margin: 0 auto

    .attrs
    

        &__header
            display: none

        &__title
            display: flex
            justify-content: space-between
            align-items: center
            font-size: 15px
            font-weight: 400
            line-height: 20px
            height: 45px
            border-top: solid 1px rgba(0, 0, 0, 0.1)
            padding: 12px 0

            &:after
