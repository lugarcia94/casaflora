.modal
    position: fixed
    top: 0
    left: 0
    z-index: 200
    display: block !important
    width: 100%
    height: 100%
    background: rgba(0, 0, 0, 0.5)
    opacity: 0
    visibility: hidden
    transition: transition[0]

    > div:last-child, > form
        position: fixed
        top: 50%
        left: 50%
        width: 600px
        height: 600px
        max-width: 100%
        max-height: 100%
        padding: 40px 0 0
        background: #fff
        transform: translate(-50%, -50%) scale(0.5)
        transition: transition[0]

    > form
        padding: 50px 30px 30px

    > div:last-child form
        display: block
        width: 100%
        height: 100%
        overflow-y: auto
        padding: 30px

    &[aria-hidden=false]
        opacity: 1
        visibility: visible

        > div:last-child, > form
            transform: translate(-50%, -50%) scale(1)

    &-header
        position: fixed
        top: 0
        left: 0
        display: flex
        justify-content: space-between
        align-items: stretch
        width: 100%

        h3
            flex: 1 1 auto
            order: -1
            display: flex
            justify-content: flex-start
            align-items: center
            padding-left: 30px
            font-size: 16px
            font-weight: bold
            text-transform: uppercase
            background: color[0]
            color: #fff

    .close
        display: flex
        justify-content: center
        align-items: center
        width: 40px
        height: 40px
        padding: 0
        border: 0
        background: color[0]
        color: #fff
        font-size: 0
        text-transform: uppercase
        line-height: 1

        &:before
            content: '+'
            font-size: 32px
            margin-top: -4px
            transform: rotate(45deg)

    #profile-submit, .btn-link
        @extend $buttonAccount
        margin: 10px 0
        border: 0

    #profile-submit
        background: color[0]
        color: #fff

    .btn-link
        margin-left: 10px

