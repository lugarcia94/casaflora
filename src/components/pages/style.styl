
.pages
    display: flex
    justify-content: center
    margin: 15px 0

    .first, .last
        display: none

    .page-number, .previous, .next
        display: flex
        justify-content: center
        align-items: center
        width: 31px
        height: 31px
        border-radius: 7px
        margin: 1px
        font-size: 12px
        background: #fff
        color: #4F4F4F
        overflow: hidden

        &.pgCurrent, &:hover
            border: 2px solid #CFB486;
            box-sizing: border-box;
            border-radius: 5px;

    .previous, .next
        font-size: 0

        &:before
            font-size: 15px

    .previous
        &:before
            font-icon: url('./svg/next.svg')
            transform: scale(-1) 
            color: #CFB486

 

    .next
        &:before
            font-icon: url('./svg/next.svg') 
            color: #CFB486
 

.vitrine
    margin-top: 30px
    margin-bottom: 30px

.searchResultsTime
    margin-top: 15px
    font-size: 13px
    font-weight: regular
    text-align: center
    color: color[1]

    .resultado-busca-termo, .resultado-busca-tempo
        display: none

.main
    .sub
        margin-top: 15px

.vitrine
    + .searchResultsTime
        + .sub .orderBy
            display: none

.resultado-busca-filtro
    font-size: 13px
    font-weight: regular

    .compare, .filterBy
        display: none

    .orderBy
        white-space: nowrap
        text-align: center

        label, .SumoSelect
            display: inline-block
            vertical-align: middle

        >label
            display: none

        .SumoSelect
            max-width: 150px

            > .CaptionCont
                border: 2px solid #CFB486
                border-radius: 5px

                > span
                    height: 23px
                    line-height: 23px
                    font-weight: 700
                    text-transform: uppercase
                    color:#4F4F4F

                > label
                    display: flex
                    justify-content: center
                    align-items: center

                    &:before
                        font-icon: url('./svg/down.svg')
                        font-size: 7px
                        color: #CFB486

                    > i
                        display: none

            > .optWrapper
                width: calc(100% + 100px)
                right: 0!important
                left: auto


.searchResultsTime
