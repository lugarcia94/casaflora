.minibanner
    text-align: center 
    background: #333333
    padding: 50px 0

    

    h2
        font-weight: 500;
        font-size: 28px;
        line-height: 30px; 
        text-align: center   
        color: #fff 

    >a
        font-weight: bold
        font-size: 14px
        text-transform: uppercase
        line-height: 40px
        color: #FF4F00
        display: flex
        align-items: center
        justify-content: center

        &:after
            content: ''
            background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAA4AAAAQCAMAAAARSr4IAAAAJFBMVEUAAAD/UAD/UAD/SgD/UAD/TAD/UAD/TwD/TwD/TwD/TwD/TwDlbkdBAAAAC3RSTlMAECAwMFBQv8/f78SDPS8AAAA3SURBVHjance5AYAwDANA2bxC+88L5E9aX3coNsPk0D3fuN5DZ/dIFzQjvNkpOhq7Y8ORNjnrPjWYAqXXtA8TAAAAAElFTkSuQmCC')
            width 15px
            height 15px
            display: block
            margin-left 5px

    .minibanner__category
        display: flex
        align-items: center
        max-width 1320px
        padding: 0 15px
        justify-content: center
        margin: 30px auto

        @media(max-width : 991px)
            flex-wrap: wrap
            

            .box-banner
                width 30%
                min-width: 0
                margin-bottom 15px

    @media(max-width : 991px)
        padding: 30px 0
        .slick-dots
            margin: 0 auto
            margin-top: 38px

    &__container
        max-width: 1320px
        width: 100%
        padding: 0 15px
        margin: 0 auto
        display: flex
        align-items: center
        justify-content: space-between 
        padding-bottom: 30px
    .box-banner
        overflow hidden
        min-width: 141px
    img
        display block
        margin: 0 auto
        transition: all .3s
        &:hover
            transform: scale(1.05)
    .box-banner
        padding: 0 5px

section.minibanner__full 
    width: 100%
    text-align: center

section.minibanner-inst 
    text-align: center
    margin: 30px 0

.minibanner-mid
    background: #CAD7CB
    padding: 90px 0

    @media(max-width : 991px)
        padding: 30px 0

    &__container
        max-width: 1120px
        width: 100%
        padding: 0 15px
        margin: 0 auto
        display: flex
        align-items: flex-end
        justify-content: space-around

        @media(max-width : 767px)
            flex-wrap: wrap

        
    .box-banner
        padding: 5px
        overflow hidden
    img
        margin-left: auto
        transition: all .3s
        &:hover
            transform: scale(1.05)

.minibanner-down
    padding-bottom: 60px
    padding-top: 20px
    @media(max-width : 991px)
        padding: 38px 0
    &__container
        max-width: 1320px
        width: 100%
        padding: 0 15px
        margin: 0 auto
        overflow hidden
        
    img
        transition: all .3s
        &:hover
            transform: scale(1.05)

.banner--full

    .slick-list
        img
            margin: 0 auto
    
    .slick-dots
        position: absolute
        bottom: 20px 
        left: 50%
        transform: translateX(-50%)

        button
            width: 20px
            height: 20px
            padding: 0
            border: 0
            border-radius: 50%
            font-size: 0
            background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAKCAMAAAC67D+PAAAAMFBMVEUAAAD///////////////////////////////////////////////////////////87TQQwAAAAEHRSTlMACBAYICgwOEBIUGBocHiA65quNAAAAD9JREFUeNpFi4MBAEEMBOesoP9q34izoK1RCG2uhLi72jE6y5+o9PeMBLuvCfE5F4xP8NsaepLn6JTZM5Qx4w77/QSGCWNwPQAAAABJRU5ErkJggg==");
            margin: 0 6px;
            background-repeat: no-repeat;
            background-position: center;

        .slick-active
            button
                width 20px
                height 20px
                background: url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAKCAMAAAC67D+PAAAAOVBMVEUAAAD///////////////////////////////////////////////////////////////////////8KOjVvAAAAE3RSTlMADRkmM0BMTVlmc3+AjJmmsr/MsfgTiQAAAEBJREFUeNo1ioMBwEAMRd/ZSPbftW74RZ7VY/IYBr1qynUiW7/y1B8arLyogP3ECu2DYlh/NnBH+35g6tlBqtWeKlgFVeSOMJUAAAAASUVORK5CYII=");
                background-repeat: no-repeat;
                background-position: center;

    .banner__container 
        display: flex 
        justify-content: center
        overflow: hidden
    
    .box-banner 
        flex: 0 0 100% 

    &-mobile 
        display: block

    @media(max-width : 991px)
        // img
        //     display: block
        //     position: relative
